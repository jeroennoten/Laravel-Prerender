{
    "name": "nutsweb/laravel-prerender",
    "description": "Laravel middleware for prerendering javascript-rendered pages on the fly for SEO",
    "keywords": ["laravel", "prerender"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen Noten",
            "email": "jeroennoten@me.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "symfony/psr-http-message-bridge": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Nutsweb\\LaravelPrerender\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nutsweb\\LaravelPrerender\\LaravelPrerenderServiceProvider"
            ]
        }
    }
}
